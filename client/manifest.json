
{
  "name": "Phishing Detector",
  "manifest_version": 3,
  "version": "0.1",
  "description": "Attempts to use various techniques to detect phishing websites",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "desktopCapture"
  ],
  "host_permissions": [
    "https://www.google.com/*",
    "http://24.125.126.126/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
   "action": {
    "default_title": "Does a thing when you do a thing",
    "default_popup": "popup.html",
    "default_icon": {
      "32": "icons/fish.png"
    }
  }
}